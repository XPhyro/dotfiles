    $TERMINAL -e neomutt; statbarsetmail
ctrl + alt + r; ctrl + alt + r
    $TERMINAL -e ranger
ctrl + alt + b; ctrl + alt + b
    $TERMINAL -e bluetoothctl
ctrl + alt + s; ctrl + alt + s
    flameshot gui
ctrl + alt + o; ctrl + alt + {b,i,g,m,c}
    {google-chrome-stable,google-chrome-stable --incognito,steam,spotify,discord}

